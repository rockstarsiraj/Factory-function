function createCar(color,brand){
    return{
        color:color,                    #function name should follow camelcase in naming function
        brand:brand,
        start:function(){
            console.log("started");
        }
    };
}
let car1=createCar("blue","Audi");
let car2=createCar("red","Tata");
console.log(car1);
console.log(car2);
### shorthand notations for above code
function createCar(color,brand){
    return{
        color,   #removed  color:color like thid
        brand,
        start(){   #removed function() here
            console.log("started");
        }
    };
}
let car1=createCar("blue","Audi");
let car2=createCar("red","Tata");
console.log(car1);
console.log(car2);
